{
  "rules": {
    ".write": false,

    "transactions": {
      "$uid": {
        ".indexOn": [
          "id"
        ],
        ".read": "auth.uid == $uid",

        "$tid": {

        }
      }
    },

    "users": {
      ".indexOn": [
        "accountId"
      ],
      ".read": false,

      "$uid": {
        "accountId": {
          ".validate": "newData.isString() && newData.val().length > 0"
        }
      }
    }
  }
}
